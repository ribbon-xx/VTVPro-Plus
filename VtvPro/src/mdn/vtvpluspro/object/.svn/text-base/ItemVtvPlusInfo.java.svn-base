package mdn.vtvpluspro.object;


public class ItemVtvPlusInfo {
	private int typeItem; //0_channel, 1_vod, 2_eposide, 3_ads
	private String id;
	private String name;
	private String view;
	private String countLike;
	private String streamId;
	private String streamUrl;
	private boolean favorites;
	private boolean isLike;
	
	public ItemVtvPlusInfo() {
		this.isLike = false;
		this.favorites = false;
	}

	public int getTypeItem() {
		return this.typeItem;
	}

	public void setTypeItem(int typeItem) {
		this.typeItem = typeItem;
	}

	public String getId() {
		return id;
	}

	public void setId(String id) {
		this.id = id;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public String getView() {
		return view;
	}

	public void setView(String view) {
		this.view = view;
	}

	public String getStreamId() {
		return streamId;
	}

	public void setStreamId(String streamId) {
		this.streamId = streamId;
	}

	public String getCountLike() {
		return countLike;
	}

	public void setCountLike(String like) {
		this.countLike = like;
	}

	public String getStreamUrl() {
		return streamUrl;
	}

	public void setStreamUrl(String streamUrl) {
		this.streamUrl = streamUrl;
	}

	public boolean isFavorites() {
		return favorites;
	}

	public void setFavorites(boolean favorites) {
		this.favorites = favorites;
	}

	public boolean isLike() {
		return isLike;
	}

	public void setLike(boolean isLike) {
		this.isLike = isLike;
	}
}
